{
	"extends": [
		"@oly_op/eslint-config-react",
		"../../../.eslintrc.json"
	],
	"parserOptions": {
		"project": "./tsconfig.eslint.json"
	},
	"rules": {
		"jsx-a11y/tabindex-no-positive": "off",
		"jsx-a11y/anchor-is-valid": [
			"error",
			{
				"specialLink": ["to"],
				"aspects": ["noHref"],
				"components": ["Link"]
			}
		]
	}
}