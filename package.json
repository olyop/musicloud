	"scripts": {
		"start": "clear && npm-run-all -p dev:**",
		"authenticator": "clear && npm-run-all -p dev:authenticator:*",
		"player": "clear && npm-run-all -p dev:authenticator:* dev:player:*",
		"uploader": "clear && npm-run-all -p dev:authenticator:* dev:uploader:*",
		"devNope:player:codegen": "graphql-codegen-esm -w",
		"dev:player:worker": "cross-env TS_NODE_PROJECT='webpack/tsconfig.json' NODE_OPTIONS='-r dotenv/config --loader ts-node/esm' webpack -w -c webpack/player/worker.ts",
		"dev:player:client": "cross-env TS_NODE_PROJECT='webpack/tsconfig.json' NODE_OPTIONS='-r dotenv/config --loader ts-node/esm' webpack serve -c webpack/player/client.ts",
		"dev:player:server": "cross-env TS_NODE_PROJECT='src/player/server/tsconfig.json' NODE_OPTIONS='-r dotenv/config' nodemon --config src/player/server/nodemon.json src/player/server/src/index.ts",
		"devNope:uploader:client": "cross-env TS_NODE_PROJECT='webpack/tsconfig.json' NODE_OPTIONS='-r dotenv/config --loader ts-node/esm' webpack serve -c webpack/uploader/client.ts",
		"devNope:uploader:server": "cross-env TS_NODE_PROJECT='src/uploader/server/tsconfig.json' NODE_OPTIONS='-r dotenv/config' nodemon --config src/uploader/server/nodemon.json src/uploader/server/src/index.ts",
		"dev:authenticator:client": "cross-env TS_NODE_PROJECT='webpack/tsconfig.json' NODE_OPTIONS='-r dotenv/config --loader ts-node/esm' webpack serve -c webpack/authenticator/client.ts",
		"dev:authenticator:server": "cross-env TS_NODE_PROJECT='src/authenticator/server/tsconfig.json' NODE_OPTIONS='-r dotenv/config' nodemon --config src/authenticator/server/nodemon.json src/authenticator/server/src/index.ts",
		"delete-build": "del-cli build",
		"build": "clear && npm-run-all delete-build build:**",
		"build-player": "clear && npm-run-all delete-build build:player:**",
		"buildNope-uploader": "clear && npm-run-all delete-build build:uploader:**",
		"build-authenticator": "clear && npm-run-all delete-build build:authenticator:**",
		"buildNope:player:codegen": "graphql-codegen",
		"build:player:server:copy": "copy-and-watch 'src/player/server/src/**/!(*.ts)' build/player",
		"build:player:server:typescript": "tsc -p src/player/server/tsconfig.json",
		"build:player:worker:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm\" webpack -c webpack/player/worker.ts",
		"build:player:client:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm\" webpack -c webpack/player/client.ts",
		"buildNope:uploader:server:copy": "copy-and-watch 'src/uploader/server/src/**/!(*.ts)' build/uploader",
		"buildNope:uploader:server:typescript": "tsc -p src/uploader/server/tsconfig.json",
		"buildNope:uploader:client:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm\" webpack -c webpack/uploader/client.ts",
		"build:authenticator:server:copy": "copy-and-watch 'src/authenticator/server/src/**/!(*.ts)' build/authenticator",
		"build:authenticator:server:typescript": "tsc -p src/authenticator/server/tsconfig.json",
		"build:authenticator:client:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm\" webpack -c webpack/authenticator/client.ts",
		"host": "clear && npm-run-all -p host-*",
		"host-player": "clear && node -r dotenv/config build/player",
		"hostNope-uploader": "clear && node -r dotenv/config build/uploader",
		"host-authenticator": "clear && node -r dotenv/config build/authenticator",
		"sync": "git checkout . && git remote update && git pull",
		"push": "git add -A && git commit -m \"Update\" && git push",
		"test": "npm run build && npm run host",
		"prepublish-common": "clear && del-cli src/common/build",
		"publish-common": "cd src/common && tsc && npm version patch && npm publish && cd ../..",
		"postpublish-common": "del-cli src/common/build && npm i @oly_op/musicloud-common@latest",
		"script:aws:create-bucket": "node --loader ts-node/esm -r dotenv/config scripts/aws/create-bucket",
		"script:algolia:send-data": "node --loader ts-node/esm -r dotenv/config scripts/algolia/send-data",
		"script:algolia:create-index": "node --loader ts-node/esm -r dotenv/config scripts/algolia/create-index",
		"script:postgresql:create-schema": "node --loader ts-node/esm -r dotenv/config scripts/postgresql/create-schema"
	},
