	"scripts": {
		"start": "npm-run-all init init:* -p dev:**",
		"player": "npm-run-all init init:player -p dev:player:**",
		"uploader": "npm-run-all init init:uploader -p dev:uploader:**",
		"authenticator": "npm-run-all init init:authenticator -p dev:authenticator:**",
		"init": "clear && del-cli build",
		"init:player": "npm-run-all build:player:server:copy build:player:server:typescript",
		"init:uploader": "npm-run-all build:uploader:server:copy build:uploader:server:typescript",
		"init:authenticator": "npm-run-all build:authenticator:server:copy build:authenticator:server:typescript",
		"dev:player:server:copy": "copy-and-watch 'src/player/server/src/**/!(*.ts)' build/player --watch",
		"dev:player:server:typescript": "tsc -p src/player/server/tsconfig.json -w",
		"dev:player:server:nodemon": "nodemon --no-warnings --experimental-specifier-resolution=node -r dotenv/config build/player",
		"dev:player:worker:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm --no-warnings --experimental-specifier-resolution=node\" webpack -w -c webpack/player/worker.ts",
		"dev:player:client:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm --no-warnings --experimental-specifier-resolution=node\" webpack serve -c webpack/player/client.ts",
		"dev:uploader:server:copy": "copy-and-watch 'uploader/server/src/**/!(*.ts)' build/uploader --watch",
		"dev:uploader:server:typescript": "tsc -p src/uploader/server/tsconfig.json -w",
		"dev:uploader:server:nodemon": "nodemon --no-warnings --experimental-specifier-resolution=node -r dotenv/config build/uploader",
		"dev:uploader:client:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm --no-warnings --experimental-specifier-resolution=node\" webpack serve -c webpack/uploader/client.ts",
		"dev:authenticator:server:copy": "copy-and-watch 'authenticator/server/src/**/!(*.ts)' build/authenticator --watch",
		"dev:authenticator:server:typescript": "tsc -p src/authenticator/server/tsconfig.json -w",
		"dev:authenticator:server:nodemon": "nodemon --no-warnings --experimental-specifier-resolution=node -r dotenv/config build/authenticator",
		"dev:authenticator:client:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm --no-warnings --experimental-specifier-resolution=node\" webpack serve -c webpack/authenticator/client.ts",
		"build": "npm-run-all init build:**",
		"build-player": "npm-run-all init build:player:**",
		"build-uploader": "npm-run-all init build:uploader:**",
		"build-authenticator": "npm-run-all init build:authenticator:**",
		"build:player:server:copy": "copy-and-watch 'src/player/server/src/**/!(*.ts)' build/player",
		"build:player:server:typescript": "tsc -p src/player/server/tsconfig.json",
		"build:player:worker:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm --no-warnings --experimental-specifier-resolution=node\" webpack -c webpack/player/worker.ts",
		"build:player:client:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm --no-warnings --experimental-specifier-resolution=node\" webpack -c webpack/player/client.ts",
		"build:uploader:server:copy": "copy-and-watch 'src/uploader/server/src/**/!(*.ts)' build/uploader",
		"build:uploader:server:typescript": "tsc -p src/uploader/server/tsconfig.json",
		"build:uploader:client:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm --no-warnings --experimental-specifier-resolution=node\" webpack -c webpack/uploader/client.ts",
		"build:authenticator:server:copy": "copy-and-watch 'src/authenticator/server/src/**/!(*.ts)' build/authenticator",
		"build:authenticator:server:typescript": "tsc -p src/authenticator/server/tsconfig.json",
		"build:authenticator:client:webpack": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm --no-warnings --experimental-specifier-resolution=node\" webpack -c webpack/authenticator/client.ts",
		"host": "clear && npm-run-all -p host:**",
		"host:player": "node --no-warnings --experimental-specifier-resolution=node -r dotenv/config build/player",
		"host:uploader": "node --no-warnings --experimental-specifier-resolution=node -r dotenv/config build/uploader",
		"host:authenticator": "node --no-warnings --experimental-specifier-resolution=node -r dotenv/config build/authenticator",
		"bump-version": "npm run version minor --no-git-tag-version",
		"sync": "git checkout . && git remote update && git pull",
		"push": "git add -A && git commit -m \"Update\" && git push",
		"prepublish-common": "clear && del-cli src/common/build",
		"publish-common": "cd src/common && tsc && npm version patch && npm publish && cd ../..",
		"postpublish-common": "del-cli src/common/build && npm i @oly_op/musicloud-common@latest",
		"script:seo:build-sitemap": "node --no-warnings --loader ts-node/esm -r dotenv/config scripts/seo/build-sitemap",
		"script:aws:create-bucket": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/aws/create-bucket",
		"script:algolia:send-data": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/send-data",
		"script:algolia:create-index": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/create-index",
		"script:algolia:generate-api-key": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/generate-api-key",
		"script:postgresql:create-schema": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/postgresql/create-schema",
		"script:postgresql:delete-schema": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/postgresql/delete-schema",
		"docker:player:build": "docker build . -t olyop/musicloud",
		"docker:player:push": "docker push olyop/musicloud",
		"docker:player:host": "docker run -p 3000:3000 -d olyop/musicloud",
		"docker-build-host-player": "npm-run-all init docker:player:build docker:player:host",
		"docker-build-push-player": "npm-run-all init docker:player:build bump-version docker:player:push"
	},
