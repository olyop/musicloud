	"engines": {
		"node": ">=17"
	},
	"scripts": {
		"start": "npm run player",
		"build-player": "npm-run-all init build:player:*",
		"build-upload": "npm-run-all init build:upload:*",
		"player": "npm-run-all init-player -p devel:player:**",
		"upload": "npm-run-all init-upload -p devel:upload:**",
		"init-player": "npm-run-all init kill-ports:player build:player:server:babel build:player:server:typescript",
		"init-upload": "npm-run-all init kill-ports:upload build:upload:server:babel build:upload:server:typescript",
		"devel:player:server:babel": "babel player/server -D --quiet -d build/player -w",
		"devel:player:server:typescript": "tsc -p player/server/tsconfig.json -w",
		"devel:player:server:nodemon": "nodemon --experimental-specifier-resolution=node -r dotenv/config build/player",
		"devel:player:client:webpack": "cross-env NODE_OPTIONS=\"--experimental-specifier-resolution=node -r dotenv/config\" webpack serve -c player/client/webpack.config.ts",
		"devel:player:worker:webpack": "cross-env NODE_OPTIONS=\"--experimental-specifier-resolution=node -r dotenv/config\" webpack -c player/service-worker/webpack.config.ts",
		"build:player:server:babel": "babel player/server -D --quiet -d build/player",
		"build:player:server:typescript": "tsc -p player/server/tsconfig.json",
		"build:player:client:webpack": "cross-env NODE_OPTIONS=\"-r dotenv/config\" webpack -c player/client/webpack.config.ts",
		"build:player:worker:worker": "cross-env NODE_OPTIONS=\"-r dotenv/config\" webpack -c player/service-worker/webpack.config.ts",
		"devel:upload:server:babel": "babel upload/server -D --quiet -d build/upload -w",
		"devel:upload:server:typescript": "tsc -p upload/server/tsconfig.json -w",
		"devel:upload:server:nodemon": "nodemon --experimental-specifier-resolution=node -r dotenv/config build/upload",
		"devel:upload:client:webpack": "cross-env NODE_OPTIONS=\"--experimental-specifier-resolution=node --openssl-legacy-provider -r dotenv/config\" webpack serve -c upload/client/webpack.config.ts",
		"build:upload:server:babel": "babel upload/server -D --quiet -d build/upload",
		"build:upload:server:typescript": "tsc -p upload/server/tsconfig.json",
		"build:upload:client:webpack": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider -r dotenv/config\" webpack -c upload/client/webpack.config.ts",
		"host:player": "node --experimental-specifier-resolution=node -r dotenv/config build/player",
		"host:upload": "node --experimental-specifier-resolution=node -r dotenv/config build/upload",
		"build-host-player": "npm-run-all build-player host:player",
		"build-host-upload": "npm-run-all build-upload host:upload",
		"sync-build-host-player": "npm-run-all sync build-host-player",
		"sync-build-host-upload": "npm-run-all sync build-host-upload",
		"sync-install-build-host-player": "npm run sync && npm i --legacy-peer-deps && npm run build-host-player",
		"sync-install-build-host-upload": "npm run sync && npm i --legacy-peer-deps && npm run build-host-upload",
		"delete-build": "rm -rf build",
		"init": "clear && npm-run-all delete-build",
		"kill-ports:player": "kill-port 3000 3001",
		"kill-ports:upload": "kill-port 3002 3003",
		"sync": "git checkout . && git remote update && git pull",
		"push": "git add -A && git commit -m \"Update\" && git push",
		"prepublish-common": "clear && tsc -p packages/tsconfig.common.json && cd packages/common",
		"publish-common": "npm version patch && npm publish",
		"postpublish": "rm -rf types globals.d.ts globals.js metadata.d.ts metadata.js options.d.ts options.js && cd ../.. && npm i -S -P @oly_op/music-app-common@latest",
		"script:aws:create-bucket": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/aws/create-bucket",
		"script:algolia:send-data": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/send-data",
		"script:algolia:create-index": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/create-index",
		"script:algolia:generate-api-key": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/generate-api-key",
		"script:postgresql:delete-schema": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/postgresql/delete-schema",
		"docker:player:build": "docker build . -t olyop/musicloud",
		"docker:player:push": "docker push olyop/musicloud",
		"docker:player:host": "docker run -p 3000:3000 -d olyop/musicloud",
		"docker-build-host-player": "npm-run-all init docker:*",
		"docker-build-push-player": "npm-run-all init docker:player:build docker:player:push"
	},
