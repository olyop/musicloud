{
	"type": "module",
	"name": "music-app",
	"license": "UNLICENSED",
	"description": "music-app",
	"author": {
		"name": "Oliver Plummer",
		"email": "oliver.plummer@outlook.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/olyop/music-app.git"
	},
	"engines": {
		"node": ">=17"
	},
	"scripts": {
		"build": "npm-run-all init build:*",
		"start": "npm-run-all init kill-ports build:player:typescript build:upload:typescript build:player:babel build:upload:babel -p dev:*:*",
		"player": "npm-run-all init kill-ports:player build:player:typescript build:player:babel -p dev:player:*",
		"upload": "npm-run-all init kill-ports:upload build:upload:typescript build:upload:babel -p dev:upload:*",
		"dev:player:babel": "babel src/player/server -D --quiet -w -d build/player",
		"dev:player:nodemon": "nodemon --experimental-specifier-resolution=node -r dotenv/config build/player",
		"dev:player:webpack": "cross-env NODE_OPTIONS=\"--experimental-specifier-resolution=node --openssl-legacy-provider -r dotenv/config\" webpack serve --config webpack.player.ts",
		"dev:player:typescript": "tsc --watch --project tsconfig.player.json",
		"dev:upload:babel": "babel src/upload/server -D --quiet -w -d build/upload",
		"dev:upload:nodemon": "nodemon --experimental-specifier-resolution=node -r dotenv/config build/upload",
		"dev:upload:webpack": "cross-env NODE_OPTIONS=\"--experimental-specifier-resolution=node --openssl-legacy-provider -r dotenv/config\" webpack serve --config webpack.upload.ts",
		"dev:upload:typescript": "tsc --watch --project tsconfig.upload.json",
		"build:player:babel": "babel -d build/player -D --quiet src/player/server",
		"build:player:webpack": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider -r dotenv/config\" webpack --config webpack.player.ts",
		"build:player:typescript": "tsc --project tsconfig.player.json",
		"build:upload:babel": "babel -d build/upload -D --quiet src/upload/server",
		"build:upload:webpack": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider -r dotenv/config\" webpack --config webpack.upload.ts",
		"build:upload:typescript": "tsc --project tsconfig.upload.json",
		"build-docker": "npm-run-all build docker:build",
		"build-player": "npm-run-all init build:player:*",
		"build-upload": "npm-run-all init build:upload:*",
		"build-docker-host": "npm-run-all build-docker docker:host",
		"build-docker-push": "npm-run-all build-docker docker:push",
		"host:player": "node --experimental-specifier-resolution=node -r dotenv/config build/player",
		"host:upload": "node --experimental-specifier-resolution=node -r dotenv/config build/upload",
		"build-host-player": "npm run build-player && npm run host:player",
		"build-host-upload": "npm run build-upload && npm run host:upload",
		"sync-build-host-player": "npm run sync && npm run build-player && npm run host-player",
		"sync-build-host-upload": "npm run sync && npm run build-upload && npm run host-upload",
		"sync-install-build-host-player": "npm run sync && npm i --legacy-peer-deps && npm run build-player && npm run host-player",
		"sync-install-build-host-upload": "npm run sync && npm i --legacy-peer-deps && npm run build-upload && npm run host-upload",
		"docker:build": "docker build -t olyop/music-app:server-latest .",
		"docker:push": "docker push olyop/music-app:server-latest",
		"docker:host": "docker run -p 80:80 olyop/music-app:server-latest",
		"init": "clear && npm-run-all delete-build fix",
		"delete-build": "del-cli build",
		"fix": "cpy src/gql node_modules/@types/gql",
		"kill-ports:player": "kill-port 3000 3001",
		"kill-ports:upload": "kill-port 3002 3003",
		"kill-ports": "npm-run-all kill-ports:*",
		"sync": "git checkout . && git remote update && git pull",
		"push": "git add -A && git commit -m \"Update\" && git push -u origin main",
		"publish-common": "tsc --outDir src/common --project tsconfig.common.json && cd src/common && npm publish && cd ../.. && npm i -S -P @oly_op/music-app-common@latest",
		"test": "clear && jest --setupFiles dotenv/config",
		"postinstall": "del node_modules/@types/webpack",
		"ec2": "sudo apt update && sudo apt upgrade -y && npm i -g npm@latest @types/node@latest && npm run sync-install-build-host-player",
		"script:aws:create-bucket": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/aws/create-bucket",
		"script:algolia:send-data": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/send-data",
		"script:algolia:create-index": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/create-index",
		"script:algolia:generate-api-key": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/generate-api-key",
		"script:postgresql:delete-schema": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/postgresql/delete-schema"
	},
