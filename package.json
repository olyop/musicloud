{
	"private": true,
	"type": "module",
	"license": "MIT",
	"version": "1.4.0",
	"name": "musicloud",
	"description": "musicloud",
	"author": {
		"name": "Oliver Plummer",
		"email": "oliver.plummer@outlook.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/olyop/musicloud.git"
	},
	"os": [
		"linux"
	],
	"engines": {
		"npm": "8",
		"node": "18"
	},
	"scripts": {
		"start": "npm-run-all init init:* -p devel:**",
		"player": "npm-run-all init init:player -p devel:player:**",
		"upload": "npm-run-all init init:upload -p devel:upload:**",
		"init:player": "npm-run-all kill-ports:player build:player:server:copy build:player:server:typescript",
		"init:upload": "npm-run-all kill-ports:upload build:upload:server:copy build:upload:server:typescript",
		"build-player": "npm-run-all init build:player:**",
		"build-upload": "npm-run-all init build:upload:**",
		"host-player": "node --experimental-specifier-resolution=node -r dotenv/config build/player",
		"host-upload": "node --experimental-specifier-resolution=node -r dotenv/config build/upload",
		"build-host-player": "npm-run-all build-player host-player",
		"build-host-upload": "npm-run-all build-upload host-upload",
		"sync-build-host-player": "npm-run-all sync build-host-player",
		"sync-build-host-upload": "npm-run-all sync build-host-upload",
		"sync-install-build-host-player": "npm run sync && npm i && npm run build-host-player",
		"sync-install-build-host-upload": "npm run sync && npm i && npm run build-host-upload",
		"devel:player:server:copy": "copy-and-watch 'player/server/src/**/!(*.ts)' build/player --watch",
		"devel:player:server:typescript": "tsc -p player/server/tsconfig.json -w",
		"devel:player:server:nodemon": "nodemon --no-warnings --experimental-specifier-resolution=node -r dotenv/config build/player",
		"devel:player:client:webpack": "cross-env NODE_OPTIONS=\"-r dotenv/config\" webpack serve -c webpack/player/client.ts",
		"devel:player:worker:webpack": "cross-env NODE_OPTIONS=\"-r dotenv/config\" webpack -w -c webpack/player/worker.ts",
		"build:player:server:copy": "copy-and-watch 'player/server/src/**/!(*.ts)' build/player",
		"build:player:server:typescript": "tsc -p player/server/tsconfig.json",
		"build:player:client:webpack": "cross-env NODE_OPTIONS=\"-r dotenv/config\" webpack -c webpack/player/client.ts",
		"build:player:worker:webpack": "cross-env NODE_OPTIONS=\"-r dotenv/config\" webpack -c webpack/player/worker.ts",
		"devel:upload:server:copy": "copy-and-watch 'upload/server/src/**/!(*.ts)' build/upload --watch",
		"devel:upload:server:typescript": "tsc -p upload/server/tsconfig.json -w",
		"devel:upload:server:nodemon": "nodemon --no-warnings --experimental-specifier-resolution=node -r dotenv/config build/upload",
		"devel:upload:client:webpack": "cross-env NODE_OPTIONS=\"-r dotenv/config\" webpack serve -c webpack/upload/client.ts",
		"build:upload:server:copy": "copy-and-watch 'upload/server/src/**/!(*.ts)' build/upload",
		"build:upload:server:typescript": "tsc -p upload/server/tsconfig.json",
		"build:upload:client:webpack": "cross-env NODE_OPTIONS=\"--experimental-specifier-resolution=node -r dotenv/config\" webpack -c webpack/upload/client.ts",
		"delete-build": "del-cli build",
		"init": "clear && npm-run-all delete-build",
		"bump-version": "npm run version minor",
		"kill-ports": "npm-run-all kill-ports:*",
		"kill-ports:player": "kill-port 3000 3001",
		"kill-ports:upload": "kill-port 3002 3003",
		"sync": "git checkout . && git remote update && git pull",
		"push": "git add -A && git commit -m \"Update\" && git push",
		"prepublish-common": "clear && del-cli packages/common/build",
		"publish-common": "cd packages/common && tsc && npm version patch && npm publish && cd ../..",
		"postpublish-common": "del-cli packages/common/build && npm i @oly_op/musicloud-common@latest --legacy-peer-deps",
		"script:seo:build-sitemap": "node --no-warnings --loader ts-node/esm -r dotenv/config scripts/seo/build-sitemap",
		"script:aws:create-bucket": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/aws/create-bucket",
		"script:algolia:send-data": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/send-data",
		"script:algolia:create-index": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/create-index",
		"script:algolia:generate-api-key": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/algolia/generate-api-key",
		"script:postgresql:delete-schema": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/postgresql/delete-schema",
		"ngrok:player": "ngrok start player --config ~/ngrok.yml --config ngrok.yml",
		"ngrok:upload": "ngrok start upload --config ~/ngrok.yml --config ngrok.yml",
		"docker:player:build": "docker build . -t olyop/musicloud",
		"docker:player:push": "docker push olyop/musicloud",
		"docker:player:host": "docker run -p 3000:3000 -d olyop/musicloud",
		"docker-build-host-player": "npm-run-all init docker:player:build docker:player:host",
		"docker-build-push-player": "npm-run-all init docker:player:build bump-version docker:player:push"
	},
